// print sum of each column in 2D array
#include <iostream>

using namespace std;

int main() {

int arr[3][3]={{1,2,3},{4,5,6},{7,8,9}};

int row = 3;

int col = 3;

int sum = 0;

for(int i=0;i<row;i++){

sum = 0;

for(int j=0;j<col;j++){

sum+= arr[i][j];

}

cout<<"Column "<<i<<" sum is "

<<sum;

cout<<endl;

}

return 0;

}